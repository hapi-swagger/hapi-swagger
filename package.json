        "json-schema-ref-parser": "^4.1.0",
        "swagger-parser": "4.0.2"
    },
    "devDependencies": {
        "babel-eslint": "^10.0.1",
        "blipp": "^3.0.0",
        "chalk": "^2.3.1",
        "code": "^5.2.0",
        "coveralls": "^3.0.0",
        "eslint": "^5.15.1",
        "eslint-config-prettier": "^4.1.0",
        "good": "^8.0.1",
        "good-console": "^7.0.1",
        "good-squeeze": "^5.0.2",
        "h2o2": "^7.0.2",
        "hapi": "^18.0.0",
        "hapi-api-version": "^1.4.0",
